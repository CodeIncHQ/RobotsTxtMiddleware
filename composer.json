{
  "name": "codeinc/robots-txt-middleware",
  "version": "1.2.1",
  "description": "A PSR-15 middleware to answers /robots.txt requests",
  "homepage": "https://github.com/CodeIncHQ/RobotsTxtMiddleware",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "arcanedev/robots": "^2.0",
    "codeinc/psr7-responses": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "guzzlehttp/psr7": "^1.4",
    "codeinc/middleware-test-kit": "^1.0"
  },
  "autoload": {
    "psr-4": {"CodeInc\\RobotsTxtMiddleware\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\RobotsTxtMiddleware\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "suggest": {
    "codeinc/middleware-dispatcher": "A PSR-15 middleware dispatcher",
    "codeinc/router": "A PSR-7 & PSR-15 router",
    "codeinc/psr7-response-sender": "A PSR-7 response sender"
  }
}
